{
  // open json editor
  "workbench.settings.editor": "json",
  // theme
  "workbench.colorTheme": "Aura Dark",
  "workbench.iconTheme": "moxer-icons",
  // fonts
  "editor.fontFamily": "Geist Mono",
  "scm.inputFontFamily": "Geist Mono",
  "terminal.integrated.fontFamily": "MesloLGS NF",
  "chat.editor.fontFamily": "Geist Mono",
  "debug.console.fontFamily": "Geist Mono",
  "editor.codeLensFontFamily": "Geist Mono",
  "notebook.output.fontFamily": "Geist Mono",
  "markdown.preview.fontFamily": "Geist Mono",
  "editor.inlayHints.fontFamily": "Geist Mono",
  // font Size
  "editor.fontSize": 14,
  "terminal.integrated.fontSize": 14,
  // ui settings
  "editor.minimap.enabled": false,
  "window.commandCenter": false,
  "editor.scrollbar.vertical": "auto",
  "editor.scrollbar.horizontal": "auto",
  "window.titleBarStyle": "custom",
  // ----------------------------------------------------------------
  "breadcrumbs.enabled": true,
  "debug.openDebug": "openOnDebugBreak",
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "editor.suggestSelection": "first",
  "editor.wordWrap": "on",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.semanticHighlighting.enabled": false,
  "editor.linkedEditing": true,
  "editor.lineHeight": 24,
  "editor.cursorBlinking": "expand",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorStyle": "line",
  "editor.cursorWidth": 2,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontLigatures": "'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'zero', 'onum'",
  "editor.renderWhitespace": "boundary",
  "editor.parameterHints.enabled": false,
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.renderControlCharacters": false,
  "editor.renderLineHighlight": "gutter",
  "editor.rulers": [
    80
  ],
  "editor.smoothScrolling": true,
  "editor.tabSize": 2,
  "editor.guides.indentation": false,
  "eslint.codeActionsOnSave.mode": "all",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "tailwind-fold.autoFold": false,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "emmet.syntaxProfiles": {
    "javascript": "jsx"
  },
  "explorer.openEditors.visible": 1,
  "explorer.sortOrder": "type",
  "explorer.compactFolders": false,
  "extensions.ignoreRecommendations": true,
  "files.trimTrailingWhitespace": true,
  "files.trimFinalNewlines": true,
  "gitlens.codeLens.authors.enabled": true,
  "gitlens.codeLens.recentChange.enabled": true,
  "gitlens.mode.active": "zen",
  "git.autofetch": true,
  "git.enableSmartCommit": true,
  "git.confirmSync": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.suggest.autoImports": false,
  "material-icon-theme.activeIconPack": "react_redux",
  "material-icon-theme.files.associations": {
    "*.sagas.ts": "Redux-store",
    "*.saga.ts": "Redux-store",
    "sagas.ts": "Redux-store",
    "*.guard.tsx": "Lock",
    "styles.ts": "Css",
